@article{Ben-David2010c,
	title = {Short-{Time} {Dynamics} of {Frictional} {Strength} in {Dry} {Friction}},
	volume = {39},
	doi = {10.1007/s11249-010-9601-9},
	abstract = {We present an experimental study of the onset of local frictional motion along a long, spatially extended interface that separates two PMMA blocks in dry frictional contact. At applied shear forces significantly below the static friction threshold, rapid precursory detachment fronts are excited, which propagate at near sound speeds along the interface. These fronts initiate from the interface edge and arrest prior to traversing the entire sample length. Along the fronts' path, we perform real-time measurements of the real contact area at every spatial point within the interface. In addition, the motion (slip) of the material adjacent to the interface is simultaneously measured at chosen locations. Upon their arrival at each spatial point along their path, these fronts instantaneously (within 4 ls) reduce the net contact area. Net slip is only initiated after this contact area reduction occurs. Slip is initially rapid and progresses at its initial velocity for a constant (60 ls) duration. Slip dynamics then undergo a sharp transition to velocities an order of magnitude slower, which remain nearly constant until slip arrest. We demonstrate that this scenario can be quantitatively explained by a model of interface weakening caused by instantaneous fracture-induced heating. Sustained rapid slip occurs in this weakened phase. Once the interface cools beneath its glass temperature the sharp transition to slow slip takes place. A similar fracture-induced weakening scenario might be expected in additional classes of materials.},
	urldate = {2019-01-04},
	journal = {Tribology Letters},
	author = {Ben-David, Odded and Cohen, Gil and Fineberg, Jay},
	year = {2010},
	keywords = {Contact mechanics, Static friction, Stick-slip, Unlubricated friction, Fracture-induced weakening, Friction mechanisms, Polymers (solid)},
	pages = {235--245},
	file = {Ben-David et al_2010_Short-Time Dynamics of Frictional Strength in Dry Friction.pdf:G\:\\My Drive\\Zotero_library\\Ben-David et al\\2010\\Ben-David et al_2010_Short-Time Dynamics of Frictional Strength in Dry Friction.pdf:application/pdf},
}

@article{Ballesta2016b,
	title = {Creep and aging of hard-sphere glasses under constant stress},
	volume = {93},
	issn = {24700053},
	doi = {10.1103/PhysRevE.93.042613},
	abstract = {We investigate the aging behavior of glassy suspensions of nearly hard-sphere colloids submitted to a constant shear stress. For low stresses, below the yield stress, the system is subject to creep motion. As the sample ages, the shear rate exhibits a power-law decrease with time with exponents that depend on the sample age. We use a combination of rheological experiments with time-resolved photon correlation spectroscopy to investigate the time evolution of the sample dynamics under shear on various time and length scales. Long-time light-scattering experiments reveal the occurrence of microscopic rearrangement events that are linked with the macroscopic strain deformation of the sample. Dynamic time sweep experiments indicate that while the internal microscopic dynamics slowdown continuously with waiting time, the storage and loss moduli are almost constant after a fast, weak decrease, resembling the behavior of quenched systems with partially frozen-in stresses.},
	number = {4},
	journal = {Physical Review E},
	author = {Ballesta, P. and Petekidis, G.},
	year = {2016},
	pages = {1--10},
}

