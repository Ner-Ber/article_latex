	file = {Bar-Sinai et al_2013_Instabilities at frictional interfaces.pdf:G\:\\My Drive\\Zotero_library\\Bar-Sinai et al\\2013\\Bar-Sinai et al_2013_Instabilities at frictional interfaces.pdf:application/pdf},
}

