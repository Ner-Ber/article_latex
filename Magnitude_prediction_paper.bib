@article{watkins_25_2016,
	title = {25 {Years} of {Self}-organized {Criticality}: {Concepts} and {Controversies}},
	volume = {198},
	issn = {1572-9672},
	shorttitle = {25 {Years} of {Self}-organized {Criticality}},
	url = {https://doi.org/10.1007/s11214-015-0155-x},
	doi = {10.1007/s11214-015-0155-x},
	abstract = {Introduced by the late Per Bak and his colleagues, self-organized criticality (SOC) has been one of the most stimulating concepts to come out of statistical mechanics and condensed matter theory in the last few decades, and has played a significant role in the development of complexity science. SOC, and more generally fractals and power laws, have attracted much comment, ranging from the very positive to the polemical. The other papers (Aschwanden et al. in Space Sci. Rev., 2014, this issue; McAteer et al. in Space Sci. Rev., 2015, this issue; Sharma et al. in Space Sci. Rev. 2015, in preparation) in this special issue showcase the considerable body of observations in solar, magnetospheric and fusion plasma inspired by the SOC idea, and expose the fertile role the new paradigm has played in approaches to modeling and understanding multiscale plasma instabilities. This very broad impact, and the necessary process of adapting a scientific hypothesis to the conditions of a given physical system, has meant that SOC as studied in these fields has sometimes differed significantly from the definition originally given by its creators. In Bak’s own field of theoretical physics there are significant observational and theoretical open questions, even 25 years on (Pruessner 2012). One aim of the present review is to address the dichotomy between the great reception SOC has received in some areas, and its shortcomings, as they became manifest in the controversies it triggered. Our article tries to clear up what we think are misunderstandings of SOC in fields more remote from its origins in statistical mechanics, condensed matter and dynamical systems by revisiting Bak, Tang and Wiesenfeld’s original papers.},
	language = {en},
	number = {1},
	urldate = {2023-06-24},
	journal = {Space Science Reviews},
	author = {Watkins, Nicholas W. and Pruessner, Gunnar and Chapman, Sandra C. and Crosby, Norma B. and Jensen, Henrik J.},
	month = jan,
	year = {2016},
	keywords = {Auroral Electrojet, Avalanche Size, Continuous Phase Transition, Sandpile Model, Solar Flare},
	pages = {3--44},
	file = {Watkins et al_2016_25 Years of Self-organized Criticality.pdf:G\:\\My Drive\\Zotero_library\\Watkins et al\\2016\\Watkins et al_2016_25 Years of Self-organized Criticality.pdf:application/pdf},
}

