	keywords = {acoustic signal identification, earthquake precursors, earthquake prediction, laboratory earthquakes, machine learning},
	pages = {9276--9282},
	file = {PDF:C\:\\Users\\user\\Zotero\\storage\\Z6K95WQ7\\full-text.pdf:application/pdf},
}

