@article{Svetlizky2017d,
	title = {Supplemental {Material}: {Frictional} {Strength} within the {Wake} of {Frictional} {Rupture} {Fronts}},
	volume = {118},
	abstract = {I. Residual stress measurements Figure S1(a) demonstrates that, to first order, local values of τ r linearly scale with local measurements of σ. In this work we concentrate on the rate dependence of τ r , which is a second order effect. This rate dependence can be easily obscured, however, by spatial variations, the loading configuration, the alignment of the blocks, small changes in the cleaning protocol and contamination of the frictional interface. To eliminate any of these effects we compare rupture events within a single stick-slip sequence. By a stick-slip sequence we mean that the blocks were aligned, pressed together by external normal stress and subsequently shear stress was quasi-statically increased, generating multiple successive rupture events. We only consider stick-slip sequences where a significant variation of rupture velocities was observed; in the sequences considered, both slow and fast rupture events were nucleated. By this method, the rate dependence of τ r /σ can be studied while any irreproducibility in the loading, alignment and cleaning are eliminated. A large diversity of rupture events can often occur spontaneously [1], or be obtained by external rupture nucleation [2]. Measurements obtained from a single stick-slip sequence are presented in Fig. S1(b). Rate dependence of τ r /σ, for each specific location, is evident, while spatial variations can easily obscure this observation. While we do not understand the origin of the spatial variations of our measurements, we can compare measurements at different locations by normalizing the measured τ r /σ by their mean value for v {\textless} 10 −1 m/s (in the range where τ r /σ is roughly rate independent). By this method various measurement locations are compared (Fig. 3(a) of the main text). Subtracting the mean value for v {\textless} 10 −1 m/s results in similar conclusions. We also emphasize that each set of measurements that included the supershear regime (blue symbols in Fig. 3(a)) also included measurements in the sub-Rayleigh regime (orange symbols in Fig. 3(a)). All measurements at each given location were normalized by the same value and therefore their relative comparison is valid.},
	urldate = {2019-01-01},
	journal = {Physical Review Letters},
	author = {Svetlizky, Ilya and Bayart, Elsa and Cohen, Gil and Fineberg, Jay},
	year = {2017},
	file = {Svetlizky et al_2017_Supplemental Material.pdf:G\:\\My Drive\\Zotero_library\\Svetlizky et al\\2017\\Svetlizky et al_2017_Supplemental Material.pdf:application/pdf},
}

@article{Rubinstein2006a,
	title = {Crack-like processes governing the onset of frictional slip},
	volume = {140},
	doi = {10.1007/s10704-006-0049-8},
	abstract = {We perform real-time measurements of the net contact area between two blocks of like material at the onset of frictional slip. We show that the process of interface detachment, which immediately precedes the inception of frictional sliding, is governed by three different types of detachment fronts. These crack-like detachment fronts differ by both their propagation velocities and by the amount of net contact surface reduction caused by their passage. The most rapid fronts propagate at intersonic velocities but generate a negligible reduction in contact area across the interface. Sub-Rayleigh fronts are crack-like modes which propagate at velocities up to the Rayleigh wave speed, V R , and give rise to an approximate 10\% reduction in net contact area. The most efficient contact area reduction (∼20\%) is precipitated by the passage of 'slow detachment fronts'. These fronts propagate at 'anomalously' slow velocities, which are over an order of magnitude lower than V R yet orders of magnitude higher than other characteristic velocity scales such as either slip or loading velocities. Slow fronts are generated, in conjunction with intersonic fronts, by the sudden arrest of sub-Ray-leigh fronts. No overall sliding of the interface occurs until either of the slower two fronts traverses the entire interface, and motion at the leading edge of the interface is initiated. Slip at the trailing edge of the interface accompanies the motion of both the slow and sub-Rayleigh fronts. We might expect these modes to be important in both fault nucleation and earthquake dynamics.},
	urldate = {2019-01-01},
	journal = {Int J Fract},
	author = {Rubinstein, Shmuel M. and Shay, Meni and Cohen, Gil and Fineberg, Jay},
	year = {2006},
	pages = {201--212},
	file = {Rubinstein et al_2006_Crack-like processes governing the onset of frictional slip.pdf:G\:\\My Drive\\Zotero_library\\Rubinstein et al\\2006\\Rubinstein et al_2006_Crack-like processes governing the onset of frictional slip.pdf:application/pdf},
}

