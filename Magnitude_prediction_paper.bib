@article{Chen2012,
	title = {Scale-{Invariant} {Correlations} in {Dynamic} {Bacterial} {Clusters}},
	doi = {10.1103/PhysRevLett.108.148101},
	abstract = {In Bacillus subtilis colonies, motile bacteria move collectively, spontaneously forming dynamic clusters. These bacterial clusters share similarities with other systems exhibiting polarized collective motion, such as bird flocks or fish schools. Here we study experimentally how velocity and orientation fluctuations within clusters are spatially correlated. For a range of cell density and cluster size, the correlation length is shown to be 30\% of the spatial size of clusters, and the correlation functions collapse onto a master curve after rescaling the separation with correlation length. Our results demonstrate that correlations of velocity and orientation fluctuations are scale invariant in dynamic bacterial clusters.},
	urldate = {2017-12-02},
	journal = {Physical Review Letters},
	author = {Chen, Xiao and Dong, Xu and Be 'er, Avraham and Swinney, Harry L and Zhang, H P},
